{
  "name": "vite-ts",
  "private": true,
  "author": "Alireza Safari <alireza.safaree@gmail.com> (http://alireza-safari.ir)",
  "license": "MIT",
  "module": "./dist/vcp.es.js",
  "description": "Ultimate Vue3 + Vite + TS base",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/vcp.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "keywords": [
    "vite base",
    "vue3 base",
    "vite + ts base",
    "vue3 + ts base"
  ],
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alireza0sfr/vite-ts-base"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "axios": "^0.27.2",
    "c8": "^7.12.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.21",
    "sinon": "^14.0.0",
    "vitepress": "^1.0.0-alpha.13",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitest/ui": "^0.23.2",
    "@vue/test-utils": "^2.0.2",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vitest": "^0.23.2",
    "vue-tsc": "^0.39.5"
  }
}
